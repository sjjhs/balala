# include <stdio.h>
# include <math.h>
int main()
{
	double area, s;
	int a, b, c;
	scanf("%d %d %d", &a, &b, &c);
	if(a + b <= c|a + c <= b|b + c <= a)
	{
		printf("不存在此三角形\n");
	}
	else 
    {
		s = (a + b + c)/2;
	    area = sqrt(s*(s - a)*(s - b)*(s - c));
        printf("该三角形的面积为%f\n", area);
	}
	return 0;
}
      
